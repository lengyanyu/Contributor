/**index.wxss**/

.container{
  height: 1500rpx;
}
.container .search{
  width: 100%rpx;
  height: 65rpx;
  padding: 12.5rpx 0rpx 12.5rpx 10rpx;
  background: #2A8CE5;
}
.container .search-left{
  width: 100%;
  margin-left: 100rpx;
  margin-right: 100rpx;
  background: #4EA3E7;
  text-align: center;
}
.container .search-left input{
  display: inline-block;
  height: 65rpx;
  font-size: 26rpx;
}
.search-placeholder{
  color: #8CCEFD;
  line-height: 20rpx;
}
.container .search .search-left image{
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  padding: 15rpx 15rpx 15rpx 20rpx;
}
.container .search .search-right{
   flex: 1; 
}
.container .search .search-right image{
  width: 45rpx;
  height: 45rpx;
  padding: 10rpx;
}

.container{
  padding: 0;
  font-size: 14rpx;
  background: #F0F4F3;
  color: #000;
}
/*feed-item part is in app.wxss for multiplexing*/