/**app.wxss**/
.container{
  height: 1500rpx;
  padding: 0;
  font-size: 14rpx;
  background: #F0F4F3;
  color: #000;
}

.container .search{
  width: 735rpx;
  height: 65rpx;
  padding: 12.5rpx 0rpx 12.5rpx 10rpx;
  background:blueviolet;
}
.container .search-left{
  flex: 8;
  background: FFFFFF;
  text-align: left;
  border: solid white 2rpx;

}
.container .search-left input{
  display: inline-block;
  height: 65rpx;
  font-size: 26rpx;
}
.search-placeholder{
  color: #8CCEFD;
  line-height: 20rpx;
}
.container .search .search-left image{
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  padding: 15rpx 15rpx 15rpx 20rpx;
}
.container .search .search-right{
  flex: 1;
}
.container .search .search-right image{

  width: 45rpx;
  height: 45rpx;
  padding: 10rpx;
}
